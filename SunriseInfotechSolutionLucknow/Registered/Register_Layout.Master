            </ul>
            <div class="content"></div>
        </div>
        <%--navbar--%>
        <nav class="navbar navbar-expand-xxl" id="navs">
            <div class="container-fluid">
                <asp:Hyperlink runat="server"  CssClass="navbar-brand nav-img" NavigateUrl="~/Registered/Userhome.aspx" style="cursor:pointer"><img src="../res/media/img/20190708_002647.png" /><label  style="cursor:pointer" class="brand-name">Sunrise Infotech Solution</label></asp:Hyperlink>
