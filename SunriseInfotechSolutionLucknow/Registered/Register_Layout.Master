                <li id="blue" onclick="changingToBlueTheme()"></li>
                <li id="green" onclick="changingToGreenTheme()"></li>
                <li id="voilet" onclick="changingToVoiletTheme()"></li>
                <li id="cyan" onclick="changingToCyanTheme()"></li>
