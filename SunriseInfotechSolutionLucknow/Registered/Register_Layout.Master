                <label class="icon" id="icon">
                    <i class="fa-solid fa-bars"></i> Menu
                </label>
                <%--responsive navbar--%>
                <div class="collapse navbar-collapse justify-content-end small_nav" id="navbarNav">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <asp:HyperLink NavigateUrl="~/Registered/Userhome.aspx" runat="server" CssClass="nav-link"><i class="fa-solid fa-house"></i> Home</asp:HyperLink>
                        </li>
                        <li class="nav-item">
                            <asp:HyperLink NavigateUrl="~/Registered/Profile.aspx" runat="server" CssClass="nav-link"><i class="fa-solid fa-id-badge"></i> Profile</asp:HyperLink>
                        </li>
                        <li class="nav-item">
                            <asp:Hyperlink runat="server" NavigateUrl="~/Registered/Certificate.aspx" CssClass="nav-link"><i class="fa-solid fa-award"></i> Certificates</asp:Hyperlink>
                        </li>
                        <li class="nav-item">
                            <asp:HyperLink NavigateUrl="~/Registered/Feedback.aspx" runat="server" CssClass="nav-link"><i class="fa-solid fa-hand-holding-heart"></i> Feedback</asp:HyperLink>
                        </li>
                        <li class="nav-item">
                            <asp:Button Text="Logout" runat="server" ValidationGroup="fix" CssClass="nav-link p-2 rounded text-light" style="background:var(--hover-bg);width:fit-content;color:white !important;" OnClick="Unnamed3_Click"/>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <%--content place holder--%>
        <div>
            <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
            </asp:ContentPlaceHolder>
        </div>
        <%--footer--%>
    <section class="footer" id="contact">
        <div class="social">
